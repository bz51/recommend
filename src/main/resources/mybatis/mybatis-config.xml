<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>

    <settings>
        <!-- 开启结果集自动映射（"_"——>驼峰命名） -->
        <setting name="mapUnderscoreToCamelCase" value="true"/>
    </settings>

    <typeAliases>
    </typeAliases>

    <typeHandlers>
        <!-- 所有枚举均使用EnumTypeHandler进行转化 -->
        <typeHandler handler="com.chaimm.rcmd.typehandler.EnumTypeHandler" javaType="com.chaimm.rcmd.entity.enumeration.BaseEnum"/>
    </typeHandlers>

</configuration>